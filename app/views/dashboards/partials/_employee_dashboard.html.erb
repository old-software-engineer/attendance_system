<% if current_employee.is_checked_in? %>
<%= form_with(url: dashboards_check_out_path, method: "post") do |form| %>
<%= form.submit t("generic.Check_Out") %>
<% end %>
<% else %>
<%= form_with(url: dashboards_check_in_path, method: "post") do |form| %>
<%= form.submit t("generic.Check_In") %>
<% end %>
<% end %>

<br><br>
